<?xml version='1.0' encoding='utf-8'?>
<widget id="freenet.cppsvr" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>资源库</name>
    <description>
        android's C++ http server 
    </description>
    <author email="wei-jiang@163.com" href="http://cordova.io">
        freelancer
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="res/magic.mgc" target="app/src/main/assets/magic.mgc" />
        <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-networkinterface" spec="^2.0.0" />
    <engine name="android" spec="^7.1.1" />
</widget>
